<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="MasterNodeConfig" type="NServiceBus.Config.MasterNodeConfig, NServiceBus.Core" />
    <section name="UnicastBusConfig" type="NServiceBus.Config.UnicastBusConfig, NServiceBus.Core" />
    <section name="Logging" type="NServiceBus.Config.Logging, NServiceBus.Core" />
    <section name="SecondLevelRetriesConfig" type="NServiceBus.Config.SecondLevelRetriesConfig, NServiceBus.Core"/>
    <section name="TransportConfig" type="NServiceBus.Config.TransportConfig, NServiceBus.Core"/>
  </configSections>
  <appSettings>
    <add key="NServiceBus/Distributor/WorkerNameToUseWhileTesting" value="Samples.Scaleout.v4.Worker2" />
  </appSettings>
  <MasterNodeConfig Node="localhost" />
  <UnicastBusConfig DistributorControlAddress="Samples.Scaleout.v4.Server.Distributor.control@localhost" DistributorDataAddress="Samples.Scaleout.v4.Server@localhost" />
  <TransportConfig MaxRetries="0" />
  <SecondLevelRetriesConfig Enabled="true"
                           TimeIncrease="00:00:10"
                           NumberOfRetries="1" />
  <Logging Threshold="Warn" />
</configuration>